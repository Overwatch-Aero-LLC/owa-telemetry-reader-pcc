			</Group>
			
			<Group name="Manual yaw control">
				<Element name="Manual yaw rate to rudder"    type="float" units="na" display="%7.4f"/>
			</Group>
			
			<Group name="Steering control">
				<Element name="Y to Vy scaling power"         type="float" units="na" display="%7.4f"/>
